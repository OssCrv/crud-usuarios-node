<!DOCTYPE html>
<html lang="en">
<head>
    
    <%- include('../partials/head'); %>




    <title>Usuarios Activos</title>
</head>
<body>

    <%- include('../partials/header'); %>

    <section class="container mt-4">
        <script>
            $(document).ready(function() {
                $('#example').DataTable();
            } );
          </script>
        <table id="example" class="table table-striped table-bordered table-sm" cellspacing="0" width="100%">
            <thead>
              <tr>
                <th scope="col">Id</th>
                <th scope="col">Nombre Completo</th>
                <th scope="col">Nombre de Usuario</th>
                <th scope="col">Fecha de Creación de la cuenta</th>
                <th scope="col">Última edición a los datos</th>
            </tr>
            </thead>
            <tbody>
    
                <!-- 
    [RowDataPacket {
        cedula: 1015196651,
        full_name: 'Adrian Enrique Sanabria Mendoza',
        user_name: 'adrian',
        pass: '$2y$08$IRtTCTFE.aphzRSCDf.u5u8GjZuoAlAkVPAo/BEImfsKFhXvxLGb2',
        rol: 'CLIENT',
        date_creation: 2021-07-01T15:26:32.000Z,
        last_update: 2021-07-01T15:26:32.000Z,
        is_active: 1}] -->

            <% for(let user of userList) { %>
                    <tr>
                        <th scope="row">  <%= user.cedula %> </th>
                        <td> <%= user.full_name %> </td>
                        <td> <%= user.user_name %> </td>
                        <td> <%= user.date_creation %> </td>
                        <td> <%= user.last_update %> </td>
                    </tr>
            <% } %>
        </tbody>
          </table>
    </section>

</body>
</html>